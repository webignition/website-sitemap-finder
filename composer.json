{
    "name": "webignition/website-sitemap-finder",
    "description": "Finds the sitemap(.xml) for a given website",
    "keywords": ["sitemap"],
    "homepage": "https://github.com/webignition/website-sitemap-finder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "jon@webignition.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\WebsiteSitemapFinder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\Tests\\WebsiteSitemapFinder\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "webignition/url":">=0.1,<2.0",
        "webignition/robots-txt-file":"2.0",
        "webignition/internet-media-type":">=0.1,<2.0",
        "webignition/sitemap-model":"1.1",
        "guzzle/guzzle":">=3.8.1,<4",
        "webignition/absolute-url-deriver":">=1.3.3,<2.0",
        "webignition/cookie-url-matcher":">=0.1,<1.0",
        "webignition/web-resource-service":"1.0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "3.*"
    },
    "minimum-stability":"stable",
    "prefer-stable":true
}